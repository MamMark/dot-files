[user]
	name = Change Me Please
	email = nobody@example.com
[core]
	editor = emacsclient
	autocrlf = input
[alias]
	b 	= branch
	br	= branch
	c 	= config
	ci	= commit
	co	= checkout
	cp	= cherry-pick
	d	= diff
	graph	= log --graph --full-history --all --pretty=format:"%h%x09%d%x20%s"
	lp	= log -p
	r 	= remote
	rem	= remote
	st	= status
	s 	= status
	wc	= whatchanged
[branch]
	autosetupmerge = true
[pack]
	threads = 0
[push]
	default = current
